{
  "name": "eslint-plugin-pere",
  "version": "0.0.1",
  "packageManager": "pnpm@10.14.0",
  "description": "eslint plugin for pereformance enhancements",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/btea/eslint-plugin-pere.git"
  },
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch"
  },
  "keywords": [],
  "author": "btea <2356281422@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "eslint": "^9.32.0",
    "tsdown": "^0.13.3",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "eslint": "*"
  }
}
